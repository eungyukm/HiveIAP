<dependencies>
  <!-- Android dependencies are specified under the "androidPackages" element.
       Each "androidPackage" element contains the attributes of an Android
       dependency (e.g AAR, JAR reference). -->
  <androidPackages>
    <!-- Global set of repositories to search for packages.
         These repos will be searched for all packages specified by
         androidPackage. -->
    <repositories>
      <repository>https://repo.maven.apache.org/maven2</repository> 
<!--      <repository>https://s01.oss.sonatype.org/content/repositories/snapshots/</repository>-->
    </repositories>
    
    <androidPackage spec="com.com2us.android.hive:hive-hercules:24.1.2"/>
  </androidPackages>
  <iosPods>
      <sources>
          <source>https://github.com/Com2uSPlatformCorp/Hercules-iOS.git</source>
      </sources>
      
      <iosPod name="HerculesSwift" version="24.1.2"/>
  </iosPods>
</dependencies>
